<div class="d3-vis__wrapper d3-vis--smooth-graph">
  <div class="d3-vis d3-vis--time-of-day">
    <div class="d3-vis__y-axis">
      <div class="d3-vis__y-axis-line"></div>
    </div>
    <div class="d3-vis__chart">
      <!-- <div class="d3-vis__tooltip">
        <div class="d3-vis__tooltip-text"></div>
      </div> -->
    </div>
    <div class="d3-vis__x-axis">
      <div class="d3-vis__x-axis-line"></div>
    </div>
  </div>

  <div class="d3-vis__actions">
    <input type="checkbox" checked class="d3-vis__guide-checkbox" id="time-of-day-guides">
    <label for="time-of-day-guides">
      <div class="d3-vis__checkbox-image">
        <svg viewBox="0 0 14 14" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
          <polyline points="4 9 6 11 10 3"></polyline>
        </svg>
      </div>
      Show guides
    </label>
  </div>
</div>
